{
  "name": "build-week-scaffolding-node",
  "version": "1.0.0",
  "scripts": {
    "start": "node index.js",
    "server": "nodemon index.js",
    "migrate": "knex migrate:latest",
    "rollback": "knex migrate:rollback",
    "seed": "knex seed:run",
    "resetdb": "npm run rollback; npm run migrate; npm run seed",
    "migrateh": "heroku run knex migrate:latest -a potluck-planner-3-ft",
    "rollbackh": "heroku run knex migrate:rollback -a potluck-planner-3-ft",
    "databaseh": "heroku pg:psql -a potluck-planner-3-ft",
    "seedh": "heroku run knex seed:run -a potluck-planner-3-ft",
    "test": "cross-env NODE_ENV=testing jest --verbose --runInBand",
    "deploy": "git push heroku main"
  },
  "engines": {
    "node": "16.13.1"
  },
  "license": "ISC",
  "dependencies": {
    "bcryptjs": "^2.4.3",
    "cors": "2.8.5",
    "dotenv": "10.0.0",
    "express": "4.17.1",
    "helmet": "4.6.0",
    "jsonwebtoken": "^8.5.1",
    "knex": "0.95.14",
    "knex-cleaner": "1.3.1",
    "lower-case": "^2.0.2",
    "pg": "8.7.1"
  },
  "devDependencies": {
    "@types/jest": "27.0.3",
    "cross-env": "7.0.3",
    "eslint": "8.4.1",
    "jest": "27.4.4",
    "nodemon": "2.0.15",
    "supertest": "6.1.6"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/bloominstituteoftechnology/build-week-scaffolding-node.git"
  }
}
